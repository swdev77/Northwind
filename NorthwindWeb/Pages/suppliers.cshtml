@page
@model NorthwindWeb.Pages.SupplierModel
<div class="row">
    <h1 class="display-2">Suppliers</h1>
    <table class="table">
       <thead class="thead-inverse">
           <tr><th>Company name</th></tr>
       </thead> 
       <tbody>
           @foreach(string name in Model.Suppliers)
           {
               <tr><td>@name</td></tr>
           }
       </tbody>
    </table>
</div>
